class Solution {

    List<List<Integer>> ans;

    public List<List<Integer>> allPathsSourceTarget(int[][] graph) {
        //dfs é€’å½’
        ans = new ArrayList<>();
        List<Integer> currList = new ArrayList<>();
        currList.add(0);
        dfs(graph, 0, currList);
        return ans;
    }

    //é€’å½’
    //start-å½“å‰é€’å½’åˆ°çš„èŠ‚ç‚¹, currList-ä»åˆå§‹èŠ‚ç‚¹èµ°åˆ°å½“å‰èŠ‚ç‚¹æ‰€é€‰æ‹©çš„è·¯å¾„
    public void dfs(int[][] graph, int start, List<Integer> currList){
        if(start == graph.length-1){ //åˆ°è¾¾ç»ˆç‚¹
            List<Integer> right = new ArrayList<>(currList);
            ans.add(right);
            return;
        }
        int[] dst = graph[start];   //å½“å‰èŠ‚ç‚¹çš„æ‰€æœ‰åèŠ‚ç‚¹
        for(int idx : dst){
            currList.add(idx);  //é€‰æ‹©è¯¥èŠ‚ç‚¹
            dfs(graph, idx, currList);  //é€’å½’
            currList.remove(currList.size()-1); //æ’¤é”€é€‰æ‹©è¯¥èŠ‚ç‚¹
        }
    }
}
```

